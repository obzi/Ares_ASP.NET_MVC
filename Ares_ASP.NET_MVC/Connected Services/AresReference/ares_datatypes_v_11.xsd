      <xsd:documentation>Výpis z obchodního rejstříku podle ARES</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Uvod" type="uvod" />
      <xsd:element name="Zakladni_udaje" type="zakladni_udaje_OR" />
      <xsd:element name="Registrace" type="registrace" />
      <xsd:element minOccurs="0" name="Cinnosti" type="cinnosti" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Exekuce" type="texty" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Ostatni_skutecnosti" type="texty" />
      <xsd:element minOccurs="0" name="Kapital" type="kapital" />
      <xsd:element minOccurs="0" name="Statutarni_organ" type="statutarni_organ" />
      <xsd:element minOccurs="0" name="Statutarni_organ_spolecnosti" type="sos" />
      <xsd:element minOccurs="0" name="Statutarni_organ_komplementaru" type="sok" />
      <xsd:element minOccurs="0" name="Statutarni_organ_predstavenstvo" type="sop" />
      <xsd:element minOccurs="0" name="Predstavenstvo" type="predstavenstvo" />
      <xsd:element minOccurs="0" name="Statutarni_organ_zrizovatele_ZO" type="szo" />
