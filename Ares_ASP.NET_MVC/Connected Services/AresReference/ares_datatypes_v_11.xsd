  <xsd:complexType name="zakladni_udaje_RZZ">
    <xsd:annotation>
      <xsd:documentation>Základní identifikační a alokační údaje ekonomického subjektu</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ICO" type="udt:ico" />
      <xsd:element minOccurs="0" name="Druh_zarizeni" type="udt:obchodni_firma" />
      <xsd:element minOccurs="0" name="Typ_ZZ" type="typ_zdravotnickeho_zarizeni" />
      <xsd:element minOccurs="0" name="Financni_urad" type="financni_urad" />
      <xsd:element minOccurs="0" name="Datum_vzniku" type="xsd:date" />
      <xsd:element minOccurs="0" name="Datum_zaniku" type="xsd:date" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="typ_zdravotnickeho_zarizeni">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Typ_ZZ_kod" type="xsd:integer" />
      <xsd:element minOccurs="0" name="Typ_ZZ_nazev" type="udt:popis" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="dalsi_udaje">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Druh_zarizeni_kod" type="xsd:short" />
      <xsd:element minOccurs="0" name="Druh_zarizeni" type="udt:obchodni_firma" />
      <xsd:element minOccurs="0" name="Typ_ucetnictvi_kod" type="xsd:short" />
      <xsd:element minOccurs="0" name="Typ_ucetnictvi" type="udt:popis" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="odpoved_rcns">
    <xsd:sequence>
      <xsd:element name="Pomocne_ID" type="xsd:int" />
      <xsd:element minOccurs="0" name="Error" type="error_ARES" />
      <xsd:element name="Vysledek_hledani" type="vysledek_hledani" />
      <xsd:element name="Pocet_zaznamu" type="xsd:int" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Vypis_rcns" type="vypis_rcns" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="vypis_rcns">
    <xsd:annotation>
      <xsd:documentation>Výpis z Registru církví a náboženských společností v ARES 1.0.1</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Uvod" type="uvod" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="RCNS" type="zakladni_udaje_RCNS" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="zakladni_udaje_RCNS">
    <xsd:annotation>
      <xsd:documentation>Základní identifikační  údaje</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Datum_zmeny" type="xsd:date" />
      <xsd:element minOccurs="0" name="Klic_ARES" type="xsd:int" />
      <xsd:element minOccurs="0" name="Stav_subjektu_RCNS" type="udt:popis" />
      <xsd:element minOccurs="0" name="ICO" type="udt:ico" />
      <xsd:element minOccurs="0" name="Nazev_CPO" type="udt:obchodni_firma" />
      <xsd:element minOccurs="0" name="DIC" type="dic" />
      <xsd:element minOccurs="0" name="Financni_urad" type="financni_urad" />
      <xsd:element minOccurs="0" name="Typ_CNS" type="typ_CNS" />
      <xsd:element minOccurs="0" name="Zkr_statu" type="zkr_statu" />
      <xsd:element minOccurs="0" name="Nazev_statu" type="udt:nazev_statu" />
      <xsd:element minOccurs="0" name="Kod_PF" type="udt:kod_pravni_formy" />
      <xsd:element minOccurs="0" name="Nazev_PF" type="udt:nazev_pravni_formy" />
      <xsd:element minOccurs="0" name="Registrace_RCNS" type="registrace_RCNS" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Adresa" type="adresa_ARES" />
      <xsd:element minOccurs="0" name="Zvlastni_prava" type="texty" />
      <xsd:element minOccurs="0" name="Zpusob_jednani" type="texty" />
      <xsd:element minOccurs="0" name="Organy" type="organy" />
      <xsd:element minOccurs="0" name="Likvidatori" type="organy" />
      <xsd:element minOccurs="0" name="Spravci" type="organy" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Clen_svazu" type="ico" />
    </xsd:sequence>
    <xsd:attribute name="dod" type="xsd:date" />
    <xsd:attribute name="ddo" type="xsd:date" />
  </xsd:complexType>
  <xsd:simpleType name="typ_CNS">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Typ subjektu v RCNS</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Církev" />
      <xsd:enumeration value="Svaz" />
      <xsd:enumeration value="Právnická osoba" />
      <xsd:enumeration value="Církevní právnická osoba" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="registrace_RCNS">
    <xsd:annotation>
      <xsd:documentation>Informace o registraci církve</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Datum_reg_prav" type="xsd:date" />
      <xsd:element minOccurs="0" name="Cislo_reg_prav" type="cislo_registrace" />
      <xsd:element minOccurs="0" name="Datum_zrus_reg_prav" type="xsd:date" />
      <xsd:element minOccurs="0" name="Cislo_zrus_reg_prav" type="cislo_registrace" />
      <xsd:element minOccurs="0" name="Datum_vzniku" type="xsd:date" />
      <xsd:element minOccurs="0" name="Cislo_registrace" type="cislo_registrace" />
      <xsd:element minOccurs="0" name="Datum_zrus_reg" type="xsd:date" />
      <xsd:element minOccurs="0" name="Cislo_zrus_reg" type="cislo_registrace" />
      <xsd:element minOccurs="0" name="Datum_likvidace" type="xsd:date" />
      <xsd:element minOccurs="0" name="Datum_konkurzu" type="xsd:date" />
      <xsd:element minOccurs="0" name="Datum_zaniku" type="xsd:date" />
      <xsd:element minOccurs="0" name="Zpusob_vymazu" type="zpusob_vymazu" />
      <xsd:element minOccurs="0" name="Nastupce" type="udt:ico" />
      <xsd:element minOccurs="0" name="Zrizovatel" type="udt:ico" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="cislo_registrace">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">číslo registrace CNS</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="30" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="zpusob_vymazu">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Způsob výmazu CNS</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string" />
  </xsd:simpleType>
  <xsd:complexType name="organy">
    <xsd:annotation>
      <xsd:documentation>Orgány jsou angazma rozsirene o dic, stat, pr.formu, fyz.osobu v pravnicke, rodné prijmeni a rc cizince</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Organ" type="organ" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="organ">
    <xsd:annotation>
      <xsd:documentation>Kombinované angažmá -  rozsirena PO je s více FO</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ICO" type="udt:ico" />
      <xsd:element minOccurs="0" name="Obchodni_firma" type="udt:obchodni_firma" />
      <xsd:element minOccurs="0" name="Financni_urad" type="financni_urad" />
      <xsd:element minOccurs="0" name="DIC" type="dic" />
      <xsd:element minOccurs="0" name="Zkr_statu" type="zkr_statu" />
      <xsd:element minOccurs="0" name="Nazev_statu" type="udt:nazev_statu" />
      <xsd:element minOccurs="0" name="Kod_PF" type="udt:kod_pravni_formy" />
      <xsd:element minOccurs="0" name="Nazev_PF" type="udt:nazev_pravni_formy" />
      <xsd:element minOccurs="0" name="Adresa" type="adresa_ARES" />
      <xsd:element id="fyzicka_osoba_RCNS" minOccurs="0" maxOccurs="unbounded" name="Organ_FO" type="organ_FO" />
    </xsd:sequence>
    <xsd:attribute name="dod" type="xsd:date" />
    <xsd:attribute name="ddo" type="xsd:date" />
  </xsd:complexType>
  <xsd:complexType name="organ_FO">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Údaje fyzické osoby v RCNS</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Nazev_organu" type="udt:popis" />
      <xsd:element minOccurs="0" name="Poznamka" type="udt:popis" />
      <xsd:element minOccurs="0" name="Titul_pred" type="udt:titul_pred" />
      <xsd:element minOccurs="0" name="Jmeno" type="udt:jmeno" />
      <xsd:element minOccurs="0" name="Prijmeni" type="udt:prijmeni" />
      <xsd:element minOccurs="0" name="Rodne_prijmeni" type="udt:prijmeni" />
      <xsd:element minOccurs="0" name="Titul_za" type="udt:titul_za" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Adresa" type="adresa_ARES" />
    </xsd:sequence>
    <xsd:attribute name="dod" type="xsd:date" />
    <xsd:attribute name="ddo" type="xsd:date" />
  </xsd:complexType>
  <xsd:simpleType name="zkr_statu">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Zkratka státu na 3 písmena</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="3" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="chybova_odpoved">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Chybová odpověď v elementu Fault</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="faultcode" type="udt:popis" />
      <xsd:element minOccurs="0" name="faultstring" type="udt:popis" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="nace">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Klasifikace ekonomických činností (CZ-NACE) ČSÚ</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="NACE" type="kod_nace" />
      <xsd:element minOccurs="0" name="Nazev_NACE" type="nazev_nace" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="kod_nace">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">NACE</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="kod_naceSimpleType">
        <xsd:attribute name="dod" type="xsd:date" />
        <xsd:attribute name="ddo" type="xsd:date" />
        <xsd:attribute name="zdroj" type="zdroj_type" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="kod_naceSimpleType">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="6" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="nazev_nace">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Název CZ-NACE</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="240" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="kod_oboru">
    <xsd:annotation>
      <xsd:documentation xml:lang="cs">Kód oboru v kvalifikaci živnosti</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="9" />
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>